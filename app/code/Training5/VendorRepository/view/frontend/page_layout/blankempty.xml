<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_layout.xsd">
    <!--This will load containers declaration to avoid errors caused by possibe <move> instructions -->
    <update handle="empty"/>
    <!-- Removing not needed containers -->
    <remove name="before.body.end"/>
    <remove name="page.wrapper"/>
    <remove name="header-wrapper"/>
    <remove name="footer"/>
    <remove name="header.panel"/>
    <remove name="after.body.start"/>
    <!-- Moving "content" container to the root to get cms page content output -->
    <!--<move element="content" destination="root" before="-" />-->
</layout>